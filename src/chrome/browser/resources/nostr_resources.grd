      <include name="IDR_NOSTR_ICON_16" 
               file="nostr/icons/nostr_16.png" 
               type="BINDATA" />
      <include name="IDR_NOSTR_ICON_32" 
               file="nostr/icons/nostr_32.png" 
               type="BINDATA" />
      <include name="IDR_NOSTR_ICON_48" 
               file="nostr/icons/nostr_48.png" 
               type="BINDATA" />
      <include name="IDR_NOSTR_ICON_128" 
               file="nostr/icons/nostr_128.png" 
               type="BINDATA" />
    </includes>
  </release>
</grit>